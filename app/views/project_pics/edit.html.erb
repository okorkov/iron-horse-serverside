
<h3 id="welcome-message">Project Pictures</h3>

<div class="category container">

<%@info.project_pics.each do |img|%>
  <%=image_tag img, class:'header-img'%>
<%end%>

    <%=form_for @info do |f| %>
    <%= f.label :upload_project_images %>
    <%=f.file_field :project_pics, multiple:true, class:"form-control" %>
    <br>
    <%=f.submit "Update Project Images", class:"btn btn-primary btn"%>
  <%end%>
</div>